package numericData;

public class Exercise1 {
	// Add five integers and display the sum

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// Declare 5 integer variables and assign a value
		int number1 = 11;
		int number2 = 22;
		int number3 = 33;
		int number4 = 44;
		int number5 = 55;
		double number6 = 66;
		double number7 = 88;
		float number8 = 99;
		float number9 = 200;
		
		// Declare an integer variable called sum to hold the sum value
		int sum = number1 + number2 + number3 +number4 + number5;
		int average = sum/5;
		double div = number7/number6 ;
		float div2 = number9/number8 ;
	    double multiply = number1*number2*number3*number4*number5*number6*number7*number8*number9;
	    double mul2 = (number1*number2)/number3;
		
		System.out.println ("The sum is : " +sum);
		System.out.println ("The average is : " +average);
		System.out.printf ("The divide number is : %.2f" ,div);
		System.out.println ("");
		System.out.printf ("The divide float number is : %.2f" , div2);
		System.out.println ("");
		System.out.printf ("The multiply is : %.2f" , multiply);
		System.out.println ("");
		System.out.printf ("The multiply num1 and num2 divide by num3 : %.2f" , mul2);
		

	}

}
